<p id="notice"><%= notice %></p>

<h1>Farmaceuticos</h1>

<table>
  <thead>
    <tr>
      <th>Cpf</th>
      <th>Nome</th>
      <th>Endereco</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @farmaceuticos.each do |farmaceutico| %>
      <tr>
        <td><%= farmaceutico.CPF %></td>
        <td><%= farmaceutico.NOME %></td>
        <td><%= farmaceutico.ENDERECO %></td>
        <td><%= link_to 'Show', farmaceutico %></td>
        <td><%= link_to 'Edit', edit_farmaceutico_path(farmaceutico) %></td>
        <td><%= link_to 'Destroy', farmaceutico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    <% will_paginate @farmaceuticos %>
  </tbody>
</table>

<br>

<%= link_to 'New Farmaceutico', new_farmaceutico_path %>
